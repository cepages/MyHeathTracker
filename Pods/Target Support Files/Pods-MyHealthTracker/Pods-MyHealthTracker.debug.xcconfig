ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
EMBEDDED_CONTENT_CONTAINS_SWIFT = YES
FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/CareKit" "$PODS_CONFIGURATION_BUILD_DIR/ResearchKit" "$PODS_CONFIGURATION_BUILD_DIR/THSegmentedControl"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/CareKit/CareKit.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/ResearchKit/ResearchKit.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/THSegmentedControl/THSegmentedControl.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "CareKit" -framework "ResearchKit" -framework "THSegmentedControl"
OTHER_SWIFT_FLAGS = $(inherited) "-D" "COCOAPODS"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
